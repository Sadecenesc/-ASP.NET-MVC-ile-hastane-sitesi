@model OgretimUyesi

<form method="post" asp-action="DeletePOST">
	<input asp-for="Id" hidden />
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary"> Ogretım Uyesi Silme </h2>
			<hr />
		</div>
		<div class="mb-3">
			<label asp-for="Ad"></label>
			<input asp-for="Ad" disabled class="form-control" />

		</div>
		<div class="mb-3">
			<label asp-for="Soyad"></label>
			<input asp-for="Soyad" disabled class="form-control" />

		</div>
		<div class="mb-3">
			<label asp-for="Telefon"></label>
			<input asp-for="Telefon" disabled class="form-control" />

		</div>
		<div class="mb-3">
			<label asp-for="Adres"></label>
			<input asp-for="Adres" disabled class="form-control" />

		</div>
		<div class="mb-3">
			<label asp-for="Eposta"></label>
			<input asp-for="Eposta" disabled class="form-control" />

		</div>
		<div class="mb-3">
			<label asp-for="SorumluBolumId"></label>
			<input asp-for="SorumluBolumId" disabled class="form-control" />

		</div>
		<div class="mb-3">
			<label asp-for="UserId"></label>
			<input asp-for="UserId" disabled class="form-control" />

		</div>

		<button type="submit" class="btn btn-danger" style="width:180px">Sil</button>
		<a asp-controller="OgretimUyesi" asp-action="Index" style="width:150px">Listeye  Dön </a>


	</div>

</form>